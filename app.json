{
  "name": "Tzunami",
  "logo": "https://telegra.ph/file/050540d246273c5daaa94.jpg",
  "description": "A Powerful Telethon Based Virus-Spambot.",
  "keywords": ["virus", "spambot", "telegram"],
  "repository": "https://github.com/Zycho-Dev-66/Tzunami",
  "stack": "container",
  "buildpacks": [
   {
     "url": "heroku/python"
   }   
  ],        
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session string",
      "required": true
    },
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "ALIVE_PIC": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true
    },
    "LOGGER_ID": {
      "description": "Max audio duration limit for downloads (minutes).",
      "required": true,
      "value": "10"
    }
  }
}
